syn off
set nu

colorscheme morning

hi Normal ctermfg=black ctermbg=grey

set cursorline
hi CursorLine term=bold ctermbg=white ctermfg=black

hi lineNr ctermfg=black ctermbg=grey
hi cursorlineNr ctermfg=white ctermbg=blue
hi visual ctermfg=white ctermbg=black

filetype indent on
set autoindent
set tabstop=4
set shiftwidth=4
set expandtab
set softtabstop=4

set nobackup


